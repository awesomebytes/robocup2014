#JSGF V1.0;

grammar first;

public <basic> = (  ( <yes> {obj.put("yes",$.$value);} ) | ( <no> {obj.put("no",$.$value);} ) | <garbage> ) {obj.put("external","1");}; 

<yes> = yes | yeah | sure;
<no> = no;

<garbage>  = <garbage2>;

<garbage1> = _unk;
<garbage2> = <garbage1> | (_unk _unk);
<garbage3> = <garbage2> | (_unk _unk _unk);
<garbage4> = <garbage3> | (_unk _unk _unk _unk);
<garbage5> = <garbage4> | (_unk _unk _unk _unk _unk);


 
